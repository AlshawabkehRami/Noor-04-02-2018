<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >


<suite name="نماذج السلامة المدرسية">

    <test name="/مدير عام الامن والسلامة/أضافة نموذج الامن والسلامة">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
