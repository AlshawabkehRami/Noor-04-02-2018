            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.Forms.ADDSafetyForms"/>
            <class name="NoorProject.Other.NoorLogOut"/>


        </classes>

    </test>
