        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.Forms.DeleteSafetyForms"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>

